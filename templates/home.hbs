<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>FHC Buildathon Home</title>
    <link rel="stylesheet" href="/css/home.css" />
  </head>
  <body>
    <div class="navbar">
      <h1>FHC Buildathon</h1>
      <div class="navbar-links">
        {{! <a href="/signup">Home</a> }}
        <a href="/">Logout</a>
      </div>
    </div>
    <div class="main-card">
      <h1>FHC Buildathon</h1>
      <p>Welcome to the orange-themed Buildathon event!</p>
    </div>
    <div class="team-cards">
      {{#each teamMembers}}
        <div class="team-card">
          <h2>{{name}}</h2>
          <p><strong>Class:</strong> {{class}}</p>
          <p><strong>Team Name:</strong> {{teamname}}</p>
        </div>
      {{/each}}
    </div>
  </body>
</html>